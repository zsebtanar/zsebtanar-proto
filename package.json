{
  "name": "zsebtanar-firebase",
  "version": "1.0.0",
  "keywords": [
    "teaching",
    "mathematics",
    "students",
    "open-source",
    "non-profit",
    "hungarian"
  ],
  "description": "Zsebtanár egy matematika oktatást segítő interaktív weboldal magyar diákok számára",
  "homepage": "https://zsebtanar.hu",
  "bugs": "https://github.com/zsebtanar/zsebtanar-proto/issues",
  "private": true,
  "engines": {
    "node": ">=13.4.0",
    "npm": ">= 6.10.0"
  },
  "scripts": {
    "start": "npm run dev",
    "predev": "shx mkdir -p bin/functions && npm run prepare:server",
    "dev": "concurrently --kill-others --names \"server,firebase,client\" \"SERVER_ENV=emulator npm run dev:server -- --watch\" \"npm run emulate:server\" \"SERVER_ENV=emulator npm run dev:client\"",
    "dev:client": "webpack serve --config ./build/webpack.client.js",
    "dev:server": "cross-env NODE_ENV=development webpack --config ./build/webpack.server.js",
    "emulate:server": "firebase use test && firebase functions:config:get > bin/functions/.runtimeconfig.json && firebase emulators:start --only functions",
    "build": "cross-env NODE_ENV=production npm run build:server && npm run build:client",
    "postbuild": "node build/post-build",
    "build:client": "cross-env NODE_ENV=production webpack --config ./build/webpack.client.js",
    "prebuild:server": "npm run prepare:server",
    "prepare:server": "node build/prepare-server",
    "build:server": "cross-env NODE_ENV=production webpack --config ./build/webpack.server.js",
    "log:server": "firebase functions:log",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "test:e2e": "is-ci \"test:e2e:run\" \"test:e2e:dev\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "start-server-and-test start http://localhost:8080 cy:run",
    "test:e2e:dev": "start-server-and-test dev http://localhost:8080 cy:open",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "check-types": "tsc --no-emit",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|json|ts|tsx)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "validate": "concurrently --kill-others --names \"test,type-check,format-check,lint,build\\ \"npm run test\" \"npm run check-types\" \"npm run check-format\" \"npm run lint\" \"npm run build && npm run test:e2e:run\"",
    "precommit": "lint-staged",
    "predeploy": "npm run test",
    "deploy": "npm run build && firebase --project=test deploy",
    "deploy:server": "npm run build:server && ./node_modules/.bin/firebase --project=test deploy --only functions",
    "build:profile": "cross-env NODE_ENV=production webpack -p --profile --json --config ./build/webpack.client.js",
    "update-packages": "npx npm-check-updates -u"
  },
  "config": {
    "firebaseFunctionDepencies": [
      "algoliasearch",
      "body-parser",
      "cookie-parser",
      "cors",
      "express",
      "firebase-admin",
      "firebase-functions",
      "markdown-it",
      "markdown-it-katex",
      "markdown-it-kbd",
      "markdown-it-center-text",
      "pocket-lisp",
      "pocket-lisp-stdlib",
      "ramda",
      "express-json-validator-middleware",
      "fluent-json-schema"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-brands-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@sentry/browser": "^5.22.3",
    "@sentry/types": "^5.22.3",
    "@types/react-router": "^5.1.3",
    "algoliasearch": "^4.4.0",
    "body-parser": "^1.19.0",
    "classnames": "^2.2.6",
    "codemirror": "^5.57.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dot-prop-immutable": "^2.1.0",
    "downshift": "^6.0.5",
    "express": "^4.17.1",
    "express-json-validator-middleware": "^2.1.1",
    "firebase": "^8.0.0",
    "firebase-admin": "^9.3.0",
    "firebase-functions": "^3.11.0",
    "fluent-json-schema": "^2.0.3",
    "history": "^5.0.0",
    "markdown-it": "^11.0.0",
    "markdown-it-center-text": "^1.0.4",
    "markdown-it-katex": "git+https://git@github.com/maxinteger/markdown-it-katex.git#v2.3.0",
    "markdown-it-kbd": "^2.2.0",
    "pocket-lisp": "latest",
    "pocket-lisp-stdlib": "latest",
    "ramda": "^0.27.1",
    "react": "^16.13.1",
    "react-async-script": "^1.2.0",
    "react-beautiful-dnd": "^13.0.0",
    "react-dnd": "^11.1.3",
    "react-dnd-html5-backend": "^11.1.3",
    "react-dom": "^16.13.1",
    "react-feather": "^2.0.8",
    "react-ga": "^3.1.2",
    "react-google-recaptcha": "^2.1.0",
    "react-input-autosize": "^2.2.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-select": "^3.1.0",
    "react-test-renderer": "^16.13.1",
    "json-schema-to-ts": "^1.5.0"
  },
  "devDependencies": {
    "@testing-library/cypress": "^6.0.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^10.4.9",
    "@testing-library/react-hooks": "^3.4.1",
    "@types/algoliasearch": "^3.34.10",
    "@types/codemirror": "^0.0.97",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.13",
    "@types/joi": "^14.3.4",
    "@types/node": "^14.6.4",
    "@types/ramda": "^0.27.14",
    "@types/raven": "^2.5.3",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "@welldone-software/why-did-you-render": "^4.3.1",
    "awesome-typescript-loader": "^5.2.1",
    "bootstrap": "^4.5.2",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.0",
    "cypress": "^4.12.1",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-jest": "^3.0.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "firebase-tools": "^8.10.0",
    "html-webpack-harddisk-plugin": "^1.0.2",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.2.5",
    "is-ci-cli": "^2.1.2",
    "jest": "^26.4.2",
    "lint-staged": "^10.3.0",
    "mini-css-extract-plugin": "^1.2.1",
    "node-sass": "^5.0.0",
    "prettier": "^2.1.1",
    "react-axe": "^3.5.3",
    "react-hot-ts": "^0.7.3",
    "sass-loader": "^10.0.5",
    "shelljs": "^0.8.4",
    "shx": "^0.3.3",
    "source-map-loader": "^1.1.0",
    "start-server-and-test": "^1.11.3",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.0.3",
    "test-data-bot": "^0.8.0",
    "ts-jest": "^26.3.0",
    "ts-loader": "^8.0.8",
    "typescript": "^4.0.5",
    "webpack": "^5.4.0",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^4.1.0",
    "webpack-dev-server": "^3.11.0"
  }
}
